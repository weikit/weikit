{
  "name": "weikit/weikit",
  "description": "Weikit.",
  "license": "MIT",
  "authors": [
    {
      "name": "CallMeZ",
      "email": "callme-z@qq.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-gd": "*",
    "laravel/sanctum": "^2.9.0",
    "intervention/image": "^2.5.0",
    "guzzlehttp/guzzle": "^7.2.0",
    "livewire/livewire": "^2.4",
    "nwidart/laravel-modules": "^8.2.0",
    "inertiajs/inertia-laravel": "^0.4.1",
    "tightenco/ziggy": "^1.1",
    "spatie/laravel-query-builder": "^3.3.4"
  },
  "autoload": {
    "files": [
      "src/Helpers/general.php",
      "src/Helpers/user.php",
      "src/Helpers/html.php"
    ],
    "psr-4": {
      "Weikit\\": "src",
      "Weikit\\Component\\": "packages/component/src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Weikit\\Providers\\WeikitServiceProvider",
        "Weikit\\Providers\\RouteServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
